    <title>History Show</title>
</head>

<body>
    <%- include('header') %>
        <button type="button" class="buttonBack"><a href="/store/figurine" class="back gold"> Back to Store</a></button>
        <h1 class="gold center" id="gold center">Sales and purchase history</h1>
        <table class="table table-striped table-bordered table-hover">
            <thead>

                <th class="headTable">Element</th>
                <th class="headTable">Values</th>
            </thead>
            <tr>
                <td class="headTable">ID</td>
                <td>
                    <%= oneHistory.history_id %>
                </td>
            </tr>
            <tr>
                <td class="headTable">Name</td>
                <td>
                    <%= oneHistory.history_name %>
                </td>
            </tr>
            <tr>
                <td class="headTable">Price</td>
                <td>
                    <%= oneHistory.history_price %> â‚¬
                </td>
            </tr>
            <tr>
                <td class="headTable">Date</td>
                <td>
                    <%= oneHistory.history_date %>
                </td>
            </tr>
            <tr>
                <td class="headTable">State</td>
                <td>
                    <%= oneHistory.history_state %>
                </td>
            </tr>
        </table>
        <%- include('footer') %>
</body>

</html>